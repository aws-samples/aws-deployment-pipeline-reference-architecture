{
  "name": "fruit-api",
  "scripts": {
    "build": "npx cdk synth -q",
    "lint": "npx eslint --ext .ts,.tsx --fix --no-error-on-unmatched-pattern infrastructure/src infrastructure/test build-tools .projenrc.js",
    "test": "npx jest --passWithNoTests --all",
    "test:update": "npx jest --passWithNoTests --all --updateSnapshot"
  },
  "devDependencies": {
    "@types/jest": "^27.0",
    "@types/node": "^14",
    "@typescript-eslint/eslint-plugin": "^5",
    "@typescript-eslint/parser": "^5",
    "aws-cdk": "^2.52.0",
    "cdk-nag": "^2.21.32",
    "esbuild": "^0.14.50",
    "eslint": "^8",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-import-resolver-typescript": "^3.3.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^27.0",
    "jest-junit": "^13",
    "json-schema": "^0.4.0",
    "npm-check-updates": "^15",
    "projen": "^0.60.3",
    "ts-jest": "^27.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@aws-cdk/aws-synthetics-alpha": "^2.55.1-alpha.0",
    "@aws-sdk/client-cloudformation": "^3.215.0",
    "@aws-sdk/client-codedeploy": "^3.154.0",
    "@aws-sdk/client-codeguru-reviewer": "^3.137.0",
    "@aws-sdk/client-codepipeline": "^3.137.0",
    "@aws-sdk/client-ecr": "^3.226.0",
    "@aws-sdk/client-iam": "^3.202.0",
    "@aws-sdk/client-s3": "^3.215.0",
    "@aws-sdk/client-sts": "^3.159.0",
    "@aws-sdk/credential-providers": "^3.159.0",
    "@aws-sdk/shared-ini-file-loader": "^3.159.0",
    "@cdklabs/cdk-ecs-codedeploy": "^0.0.20",
    "@types/aws-lambda": "^8.10.101",
    "@types/prompts": "^2.0.14",
    "aws-cdk-lib": "^2.52.0",
    "constructs": "^10.0.5",
    "prompts": "^2.4.2",
    "yaml": "^2.1.1"
  },
  "license": "Apache-2.0",
  "version": "0.0.0",
  "jest": {
    "testMatch": [
      "<rootDir>/infrastructure/src/**/__tests__/**/*.ts?(x)",
      "<rootDir>/(infrastructure/test|infrastructure/src)/**/*(*.)@(spec|test).ts?(x)"
    ],
    "clearMocks": true,
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "lcov",
      "clover",
      "cobertura",
      "text"
    ],
    "coverageDirectory": "coverage",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "watchPathIgnorePatterns": [
      "/node_modules/"
    ],
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": "test-reports"
        }
      ]
    ],
    "preset": "ts-jest",
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.dev.json"
      }
    }
  }
}
